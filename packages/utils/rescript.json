{
  "name": "@brightidbot/utils",
  "namespace": "Utils",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "pinned-dependencies": [],
  "bs-dependencies": [
    "@rescript/core",
    "@glennsl/rescript-json-combinators",
    "@brightidbot/bot",
    "@brightidbot/shared",
    "@brightidbot/scripts",
    "@brightidbot/utils"
  ],
  "bsc-flags": [
    "-open JsonCombinators",
    "-open RescriptCore"
  ],
  "reanalyze": {
    "analysis": [
      "dce",
      "exception"
    ]
  }
}