{
  "name": "@brightidbot/web",
  "version": "0.0.1",
  "sources": {
    "dir": "app",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6-global",
    "in-source": true
  },
  "suffix": ".js",
  "jsx": { "version": 4, "mode": "classic" },
  "bs-dependencies": [
    "@rescript/react",
    "rescript-remix",
    "rescript-webapi",
    "@ryyppy/rescript-promise",
    "@brightidbot/utils",
    "@glennsl/rescript-json-combinators",
    "@brightidbot/shared",
    "rescript-nodejs"
  ],
  "pinned-dependencies": ["@brightidbot/utils", "@brightidbot/shared"],
  "bsc-flags": ["-open JsonCombinators"],
  "warnings": {
    "error": "+101"
  }
}
