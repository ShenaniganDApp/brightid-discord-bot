    "@ryyppy/rescript-promise": "^2.1.0",
    "concurrently": "^7.2.0",
    "ethers": "^5.6.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-pro-sidebar": "^0.7.1",
    "remix": "1.3.4",
    "remix-auth-discord": "^1.0.0",
    "rescript-remix": "^0.2.1",
    "rescript-webapi": "^0.2.0",
    "wagmi": "^0.3.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "autoprefixer": "^10.4.7",
